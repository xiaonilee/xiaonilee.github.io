library(ggstatsplot)
ggbetweenstats(data=a, x=Group, y=Expression)
library(ggplot2)
library(survival)
library(survminer)
install.packages('survminer')
library(survminer)
table(a)
table(a$Status)
ggsave(ExpressionGroup.png)
