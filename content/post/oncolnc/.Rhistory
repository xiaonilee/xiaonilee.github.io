fit
survfit(Surv(Days,Status)~Group,data = a)
summary(fit)
ggsurvplot(fit,conf.int = F, pval = TRUE)
ggsave('survival_R.png')
ggsurvplot(fit,palette = c("#E7B800","#2E9FDF"),
risk.table = TRUE,pval = TRUE,
conf.int = TRUE,xlab="Time in months",
ggtheme = theme_light(),
ncensor.plot=TRUE)
